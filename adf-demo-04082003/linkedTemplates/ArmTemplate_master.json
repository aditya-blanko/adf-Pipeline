{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo-04082003"
		},
		"AdventureWorksDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdventureWorksDB'"
		},
		"ProductsBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProductsBlob'"
		},
		"AdventureWorksDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "serveradfaditya.database.windows.net"
		},
		"AdventureWorksDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "aditya_db"
		},
		"AdventureWorksDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "AdityaSinghal"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo-04082003_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorksDB_password": {
						"value": "[parameters('AdventureWorksDB_password')]"
					},
					"ProductsBlob_connectionString": {
						"value": "[parameters('ProductsBlob_connectionString')]"
					},
					"AdventureWorksDB_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorksDB_properties_typeProperties_server')]"
					},
					"AdventureWorksDB_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorksDB_properties_typeProperties_database')]"
					},
					"AdventureWorksDB_properties_typeProperties_userName": {
						"value": "[parameters('AdventureWorksDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}